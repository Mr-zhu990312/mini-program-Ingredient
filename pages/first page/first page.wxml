<!--pages/first page/first page.wxml-->
<view id="box">
  <view class="page_row" bindtap="suo">
    <image style="height:60rpx;width:60rpx " src="../../images/scan.png"></image>
    <view class="search">
      <view class="df search_arr">
        <icon class="searchcion" size="20" type="search" color='C2DAD7'></icon>
        <input class="" disabled placeholder="请输入关键字" value="{{searchValue}}" />
      </view>
    </view>
    <view class="news">
      <image src="../../images/news.png" style="width:60rpx;height:60rpx"></image>
    </view>
  </view>
  <view class="locate">
    <image src='../../images/locate.png ' style='width:30rpx;height:40rpx'>
    </image>
  </view>
  <view class="loctext">{{city}}</view>
  <view class='fangshai'>防晒指数</view>
  <view class='star'>
    <image wx:for="{{array}}" wx:key="index" src='../../images/empty_heart.png' style='width:30rpx;height:30rpx;'>
    </image>
  </view>
  <view class="weather">
    <image src='../../images/weather.png ' style='width:40rpx;height:35rpx'>
    </image>
  </view>
  <view class="weatext">{{weather}}</view>
  <view class='baoshi'>保湿指数</view>
  <view class='fullstar'>
    <image wx:for="{{fullarray}}" wx:key="index" src='../../images/full_heart.png' style='width:30rpx;height:30rpx;'>
    </image>
  </view>
</view>
<swiper autoplay indicator-dots indicator-color="#F7F7F7" indicator-active-color="#82C6C7" interval="2500" circular>
  <swiper-item>
    <image mode="widthFix" src="../../images/1.jpg"></image>
  </swiper-item>
  <swiper-item>
    <image mode="widthFix" src="../../images/2.jpg"></image>
  </swiper-item>
  <swiper-item>
    <image mode="widthFix" src="../../images/3.jpg"></image>
  </swiper-item>
  <swiper-item>
    <image mode="widthFix" src="../../images/4.jpg"></image>
  </swiper-item>
</swiper>
